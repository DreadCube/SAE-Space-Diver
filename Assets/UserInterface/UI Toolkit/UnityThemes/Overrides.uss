.unity-base-dropdown__container-outer {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

Toggle #unity-checkmark {
    color: rgb(255, 0, 0);
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(202, 10, 0, 0.44);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}

Scroller {
}

.unity-base-dropdown__checkmark {
    -unity-background-image-tint-color: rgb(202, 10, 0);
}

.unity-base-dropdown__container-inner {
    background-color: rgb(0, 0, 0);
    background-image: url('project://database/Assets/UserInterface/3rdParty/Cyberpunk_RPG_UI/UI_Parts/Icon_fr_3_r.png?fileID=2800000&guid=69c1a39440607314c8c09a5ab4c268b7&type=3#Icon_fr_3_r');
    -unity-font: url('project://database/Assets/UserInterface/3rdParty/Fonts/Azonix.otf?fileID=12800000&guid=75b083ec1e94f48fdb13ae3c1f289bb8&type=3#Azonix');
    -unity-font-definition: url('project://database/Assets/UserInterface/3rdParty/Fonts/Azonix%20SDF.asset?fileID=11400000&guid=06f2868e1d20247d3901194d9a377ec0&type=2#Azonix SDF');
}

.unity-base-dropdown__container-inner .unity-base-dropdown__item {
    color: rgb(202, 10, 0);
}

.unity-base-dropdown__container-inner .unity-base-dropdown__item:hover {
    background-color: rgba(0, 0, 0, 0);
}

.unity-base-field:focus > .unity-base-field__label {
    color: rgb(202, 10, 0);
}
